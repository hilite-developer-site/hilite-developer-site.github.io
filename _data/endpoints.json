                "createdAt": "2016-12-07T18:27:37.699Z",
                "updatedAt": "2016-12-07T18:27:37.699Z",
                "id": "584854990ea7cb08007d8b3b"
              },
              "preRoll": {
                "organizationId": "584854990ea7cb08007d8b35",
                "url": "http://someurl.mov",
                "mimeType": "video/mov",
                "createdAt": "2016-12-07T18:27:37.699Z",
